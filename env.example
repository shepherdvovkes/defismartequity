# Infura API Key для подключения к Sepolia
INFURA_API_KEY=your_infura_api_key_here

# RPC URL для Sepolia (альтернатива Infura)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_api_key_here

# Приватные ключи для мультиподписи (2 аккаунта)
PRIVATE_KEY_1=your_first_private_key_here
PRIVATE_KEY_2=your_second_private_key_here

# Etherscan API Key для верификации контрактов
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# OpenZeppelin Defender API Keys
DEFENDER_API_KEY=your_defender_api_key_here
DEFENDER_API_SECRET=your_defender_api_secret_here

# Адреса V1 контрактов (заполнятся после деплоя)
DEFIMON_TOKEN_ADDRESS=
INVESTMENT_CONTRACT_ADDRESS=

# Адреса V2 контрактов (заполнятся после деплоя V2)
DEFIMON_TOKEN_V2_ADDRESS=
DEFIMON_INVESTMENT_V2_ADDRESS=

# Уведомления (опционально)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Email уведомления (опционально)
SMTP_HOST=your_smtp_host_here
SMTP_PORT=587
SMTP_USER=your_smtp_user_here
SMTP_PASS=your_smtp_password_here
ALERT_EMAIL=your_alert_email_here

# Slack уведомления (опционально)
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
